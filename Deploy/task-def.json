{
    "family": "fastapi-task",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::207567796032:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "fastapi-container",
        "image": "207567796032.dkr.ecr.ap-south-1.amazonaws.com/fastapi-backend:latest",
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80,
            "protocol": "tcp"
          }
        ],
        "essential": true
      }
    ]
  }
  